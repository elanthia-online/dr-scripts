custom_require.call(%w[drinfomon common common-travel])

settings = get_settings

@rooms = settings.bail_rooms

target_room = @rooms.shuffle.first
while target_room == Room.current.id
  target_room = @rooms.shuffle.first
end

while Room.current.id==nil
  move('south')
end
if Room.current.id==19392
  move('east')
end
fput('unhide') if hidden?
DRCT.walk_to(target_room)
